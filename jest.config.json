{
  "coverageDirectory": "coverage",
  "moduleNameMapper": {
    "^#/(.*)$": "<rootDir>/$1"
  },
  "moduleFileExtensions": [
    "js"
  ],
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "collectCoverage": false,
  "collectCoverageFrom": [
    "<rootDir>/basic/*.js"
  ],
  "setupFilesAfterEnv": ["./setup-test.js"]
}
